ion.status\">\n            <span class=\"checkmark\"></span>\n          </label>\n        </div>\n      </section>\n      <footer>\n        <div class=\"password\">{{ generatedPassword }}</div>\n        <button class=\"btn-refresh\" @click=\"refreshPassword = !refreshPassword\">refresh</button>\n      </footer>\n    </article>\n  </div>\n</template>\n\n<script>\n  export default {\n    data: function() {\n      return {\n        passwordLength: 16,\n        refreshPassword: false,\n        optiondata: [\n          {\n            name: 'lowercase',\n            status: true,\n            chars: 'abcdefghjkmnopqrstuvwxyz'\n          },\n          {\n            name: 'uppercase',\n            status: true,\n            chars: 'ABCDEFGHJKLMNOPQRSTUVWXYZ'\n          },\n          {\n            name: 'numbers',\n            status: true,\n            chars: '0123456789'\n          },\n          {\n            name: 'specialchars',\n            status: false,\n            chars: '!$%&?+*#-/'\n          }\n        ]\n      }\n    },\n    computed: {\n      charset() {\n        return [...this.optiondata]\n                .map(element => {\n                  if(element.status === true)\n                    return element.chars;\n                }).join('');\n      },\n      generatedPassword() {\n        this.refreshPassword;\n\n        return [...window.crypto.getRandomValues(new Uint32Array(this.passwordLength))]\n                .map(value => this.charset[value % this.charset.length])\n                .join('');\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .container {\n  background-color: #222222 !important\n }\n  .wrapper {\n\t display: flex;\n\t justify-content: center;\n\t align-items: center;\n\t width: 100%;\n\t height: 100vh;\n}\n .passbox {\n\t width: 550px;\n\t border-radius: 10px;\n\t padding: 0 0 10px;\n\t background-color:black;\n\t text-align: center;\n\t overflow: hidden;\n   color:white\n}\n header {\n\t padding: 10px 0;\n\t margin-bottom: 10px;\n\t background-color: #4FC08D;\n}\n header h1 {\n\t margin: 0;\n\t color: #fff;\n}\n section .slidercontainer {\n\t width: 100%;\n}\n section .slider {\n\t appearance: none;\n\t width: calc(100% - 20px);\n\t height: 8px;\n\t margin-bottom: 20px;\n\t background-color: whitesmoke;\n\t outline: none;\n\t opacity: 0.7;\n\t transition: opacity 0.2s;\n}\n section .slider:hover {\n\t opacity: 1;\n}\n section .slider::-webkit-slider-thumb {\n\t appearance: none;\n\t width: 30px;\n\t height: 30px;\n\t border: 2px solid #4FC08D;\n\t border-radius: 50%;\n\t background-color: #f1c40f;\n\t cursor: pointer;\n}\n section .options {\n\t display: flex;\n\t justify-content: space-around;\n\t align-items: center;\n\t flex-wrap: wrap;\n\t padding: 20px;\n}\n section .options .container {\n\t display: block;\n\t position: relative;\n\t width: 160px;\n\t padding-left: 35px;\n\t margin-bottom: 12px;\n\t font-size: 1.4rem;\n\t user-select: none;\n\t cursor: pointer;\n}\n section .options .container:hover input ~ .checkmark {\n\t background-color: whitesmoke;\n}\n section .options .container input {\n\t position: absolute;\n\t cursor: pointer;\n\t opacity: 0;\n\t height: 0;\n\t width: 0;\n}\n section .options .container input:checked ~ .checkmark {\n\t background-color: #4FC08D;\n}\n section .options .container input:checked ~ .checkmark:after {\n\t display: block;\n}\n section .options .checkmark {\n\t position: absolute;\n\t top: 0;\n\t left: 0;\n\t height: 25px;\n\t width: 25px;\n\t background-color: whitesmoke;\n}\n section .options .checkmark:after {\n\t content: \"\";\n\t position: absolute;\n\t display: none;\n\t left: 9px;\n\t top: 5px;\n\t width: 5px;\n\t height: 10px;\n\t border: solid #fff;\n\t border-width: 0 3px 3px 0;\n\t transform: rotate(45deg);\n}\n footer .password {\n\t font-size: 2rem;\n\t overflow-wrap: break-word;\n\t padding: 5px 0;\n\t margin-bottom: 15px;\n\t border: solid #ccc;\n\t border-width: 2px 0;\n}\n footer .btn-refresh {\n\t font-size: 2rem;\n\t border: none;\n\t border-radius: 5px;\n\t background-color: #4FC08D;\n\t color: #fff;\n\t padding: 5px 10px;\n\t outline: none;\n\t cursor: pointer;\n}\n \n</style>\n"],"sourceRoot":"src/views"}]}